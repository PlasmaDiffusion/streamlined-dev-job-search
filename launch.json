{
    "type": "chrome",
    "request": "launch",
    "name": "Debug Vite App",
    "url": "http://localhost:5173",
    "webRoot": "${workspaceFolder}/src",
    "sourceMapPathOverrides": {
      "webpack:///./src/*": "${webRoot}/*"
    },
    "sourceMaps": true,
    "runtimeArgs": ["--remote-debugging-port=9222"]
  }